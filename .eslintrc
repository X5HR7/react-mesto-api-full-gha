{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "linebreak-style": "off",
      "arrow-parens": "off",
      "object-curly-newline": "off",
      "no-console": "off",
      "comma-dangle": "off",
      "no-useless-escape": "off",
      "nonblock-statement-body-position": "off",
      "no-unused-vars": "warn",
      "curly": "off",
      "import/newline-after-import": "off",
      "no-shadow": "off",
      "no-underscore-dangle": ["error",{"allow":["_id"]}],
      "implicit-arrow-linebreak": "off",
      "function-paren-newline": "off"
  }
}
